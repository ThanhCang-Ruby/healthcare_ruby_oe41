

<div class="container-content">
  <table class="table ">
    <tr class="table-head-detail">
      <th><%= t "staff_page.id" %></th>
      <th><%= t "staff_page.service" %></th>
      <th><%= t "staff_page.money" %></th>
      <th><%= t "staff_page.time" %></th>
    </tr>
    <% @work_histories.each do |work_history| %>
      <%= render partial: "work_history", locals: {work_history: work_history} %>
    <% end %>
  </table>
  <%= will_paginate @work_histories %>
</div>

